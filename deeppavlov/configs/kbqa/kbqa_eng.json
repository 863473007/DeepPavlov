{
  "dataset_reader": {
    "class_name": "kbqa_reader",
    "data_path": "{DOWNLOADS_PATH}/wikidata_eng/"
  },
  "dataset_iterator": {
    "class_name": "data_learning_iterator"
  },
  "chainer": {
    "in": ["x"],
    "in_y": ["y"],
    "pipe": [
      {
        "config_path": "{CONFIGS_PATH}/ner/ner_ent_rel_type_lower.json",
        "in": ["x"],
        "out": ["x_tokens", "x_tags"]
      },
      {
        "in": ["x_tokens", "x_tags"],
        "out": ["entities_substr_from_ner"],
        "class_name": "entity_detection_parser",
        "what_to_parse": "tags"
      },
      {
        "in": ["x"],
        "out": ["entities_substr_from_template", "rels_from_template", "query_types"],
        "class_name": "template_matcher",
        "id": "template_m",
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng",
        "templates_filename": "templates_eng.pickle"
      },
      {
        "in": ["entities_substr_from_ner"],
        "out": ["entity_ids", "entity_confs"],
        "class_name": "entity_linker",
        "id": "linker",
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng/wikidata.db",
        "link_with_docs": true,
        "use_prefix_tree": false,
        "inverted_index_table": "inverted_index",
        "entities_list_table": "entities_list",
        "inverted_index_doc_table": "inverted_index_doc",
        "entities_list_doc_table": "entities_list_doc"
      },
      {
        "in": ["x", "entity_ids", "entity_confs", "rels_from_template"],
        "out": ["answers"],
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng/wikidata.db",
        "class_name": "kb_answer_parser_simple",
        "top_k_classes": 5,
        "lemmatize": false,
        "rule_filter_entities": true,
        "language": "eng"
      }
      
    ],
    "out": ["answers"]
  },
  "train":{
    "metrics": [
      {
        "name": "kbqa_accuracy",
        "inputs": ["y", "answers"]
      }
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/tf.txt",
      "{DEEPPAVLOV_PATH}/requirements/fasttext.txt",
      "{DEEPPAVLOV_PATH}/requirements/spelling.txt"
    ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/wikidata_rus.tar.gz",
        "subdir": "{DOWNLOADS_PATH}"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/inverted_index.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_rus"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/templates.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_rus"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/wiki_properties_id_to_name.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_rus"
      },
      {
        "url": "http://files.deeppavlov.ai/embeddings/ft_native_300_ru_wiki_lenta_nltk_word_tokenize/ft_native_300_ru_wiki_lenta_nltk_word_tokenize.bin",
        "subdir": "{DOWNLOADS_PATH}/embeddings"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/kbqa_mix_lowercase.tar.gz",
        "subdir": "{MODELS_PATH}/kbqa_mix_lowercase"
      },

      {
        "url": "http://files.deeppavlov.ai/embeddings/lenta_lower_100.bin",
        "subdir": "{DOWNLOADS_PATH}/embeddings"
      }
    ]
  }
}
